<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="horizontal"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:padding="5dp"
    app:layout_collapseParallaxMultiplier="1.0"
    android:layout_centerHorizontal="false"
    android:layout_centerInParent="false">

    <TextView android:id="@+id/patid"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="Category" />

    <TextView android:id="@+id/patname"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="Information" />

    <TextView
        android:id="@+id/dob"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="La Fecha de Nacimientio" />

    <TextView
        android:id="@+id/address"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="La Dirreción" />

    <TextView
        android:id="@+id/telephone"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="EL Numero de Telefono" />

    <TextView
        android:id="@+id/height"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="La Altura" />

    <TextView
        android:id="@+id/weight"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="El Peso" />

    <TextView android:id="@+id/gender"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="El Género" />

    <TextView android:id="@+id/marstat"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="Casado?" />

    <TextView android:id="@+id/children"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="El Numero de los Niños" />

    <TextView
        android:id="@+id/allergies"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="La Alergia" />

    <TextView
        android:id="@+id/medcond"
        android:layout_width="200dp"
        android:layout_height="60dp"
        android:gravity="center_vertical"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:textStyle="bold"
        android:textSize="22dp"
        android:textAlignment="center"
        android:textColor="#00004d"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:text="Los Condiciones de Médicas" />


</LinearLayout>